{
  "compilerOptions": {
    "strict": true,
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "target": "ESNext",
    "types": ["react/experimental", "react-dom/experimental", "node"],
    "paths": {
      "@/db/*": ["./db/*"]
    }
  },
  "include": ["./src/", "./db/"]
}
